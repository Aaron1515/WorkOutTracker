<!-- Squats template -->
<br />
<%= form_for [@user, @workout] do |f| %>
  <%= f.label :phase %>
  <%= f.select(:phase, options_for_select([
    [1,1],
    [2,2],
    [3,3],
    [4,4]])) %>

  <%= f.label :day %>
  <%= f.select(:day, options_for_select([
    [1,1],
    [2,2],
    [3,3],
    [4,4]])) %>
  <%= f.hidden_field :name, value: "squats" %>
  <%= f.hidden_field :reps, value: 10 %>
  <%= f.hidden_field :sets, value: 3 %>
  <%= f.hidden_field :weight, value: "BW" %>
  <%= f.hidden_field :rest, value: "null" %>
  <%= f.submit "Squats 10-3-BW" %>
<% end %>

